div
  .container
    .page-header
      h1.page-title {{$t('headway.page_title')}} {{headway.createdAt | moment}}
    .card.col-lg-12
      .card-body
        #dimmer.dimmer(:class="{'active': isLoading}")
          .loader
          .dimmer-content
            template(v-if="!isLoading")
              Build(v-if="headway.type === 'BUILD'", :headway="headway")
              Strength(v-else, :headway="headway")
